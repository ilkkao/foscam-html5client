{
    "rules": {
        "indent": [
            2,
            4, {
                "SwitchCase": 1
            }
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "react/jsx-uses-react": 2
    },
    "env": {
        "es6": true,
        "node": true
    },
    "globals": {
        "settings": false,
        "fetch": false,
        "window": false
    },
    "ecmaFeatures": {
        "modules": true,
        "jsx": true
    },
    "plugins": [ "react" ],
    "extends": "eslint:recommended"
}
